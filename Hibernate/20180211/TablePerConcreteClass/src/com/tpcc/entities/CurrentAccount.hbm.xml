<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tpcc.entities">
	<union-subclass name="CurrentAccount" extends="Account"
		table="current_account">
		<property name="overDraftLimit" column="over_draft_limit" />
		<property name="companyName" column="company_nm" />
	</union-subclass>
</hibernate-mapping>