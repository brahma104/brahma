<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.cs.entities">
	<class name="Loan" table="loan">
		<id name="loanNo" column="loan_no" />
		<property name="loanType" column="loan_type" />
		<property name="principle" />
		<property name="tenure" />
		<property name="interestRate" column="interest_rate" />
	</class>
</hibernate-mapping>