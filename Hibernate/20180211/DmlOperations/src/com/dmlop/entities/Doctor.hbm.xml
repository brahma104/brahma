<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.dmlop.entities">
	<class name="Doctor" table="doctor" dynamic-update="true">
		<id name="doctorNo" column="doctor_no" unsaved-value="0">
			<generator class="increment" />
		</id>
		<property name="doctorName" column="doctor_nm" />
		<property name="experience" />
		<property name="specialization" />
		<property name="qualification" />
	</class>
</hibernate-mapping>