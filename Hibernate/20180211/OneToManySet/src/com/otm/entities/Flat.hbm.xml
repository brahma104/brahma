<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.otm.entities">
	<class name="Flat" table="flat">
		<id name="flatNo" column="flat_no">
			<generator class="increment" />
		</id>
		<property name="description" />
		<property name="flatType" column="flat_type" />
		<property name="sqft" />
		<property name="facing" />
		<property name="floor" />
		<property name="status" />
		<many-to-one name="block" column="block_no"/>
	</class>
	<query name="sqlFlatsByType"><![CDATA[from Flat f where f.flatType = :flatType]]></query>
</hibernate-mapping>





















