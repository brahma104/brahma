<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.oto.entities">
	<class name="Admission" table="admission">
		<id name="admissionNo" column="admission_no">
			<generator class="increment" />
		</id>
		<property name="admissionDate" column="admission_dt" />
		<property name="candidateName" column="candidate_nm" />
		<property name="gender" column="gender" />
		<property name="dateOfBirth" column="date_of_birth" />
		<property name="degree" />
		<property name="college" column="college_nm" />
	</class>
</hibernate-mapping>